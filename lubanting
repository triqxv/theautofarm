function webhook(title, text, color)
    local url = "https://discordapp.com/api/webhooks/1132658021630218351/a0n4k-SW5Lz1293sw5y7VPdeh8n4wAbvbhRyD1EewK5-quOhYsoymEYT_bnfgqKhibj1"
    local data = {
       ["embeds"] = {
           {
               ["title"] = title,
               ["description"] = text,
               ["type"] = "rich",
               ["timestamp"] = os.date("%Y-%m-%dT%X.000Z"),
               ["color"] = tonumber(0x7269da),
               ["image"] = {
                   ["url"] = ""
               };
           }
       }
    }
 
 
    local newdata = game:GetService("HttpService"):JSONEncode(data)
 
    local headers = {
       ["content-type"] = "application/json"
    }
    request = http_request or request or HttpPost or syn.request
    local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
    local a,b = pcall(request,abcdef)
    print(a,b)
    table.foreach(b,print)
    end
 
 
for i,v in hwidpaste do
	if v == UserHwid then
	    KeySystem = false
        webhook("Script Execution", "Earlier access hwid executed script. \n hwid: ``" ..UserHwid.. "`` \n User Link: https://www.roblox.com/users/" ..game:GetService("Players").LocalPlayer.UserId.. "/profile" , tonumber(0x00ff00))
    elseif v ~= UserHwid then
        webhook("Script Earlier Access request", "Hwid request., hwid: ``" ..UserHwid.. "`` \n User Link: https://www.roblox.com/users/" ..game:GetService("Players").LocalPlayer.UserId.. "/profile" , tonumber(0x00ff00))
	end
end
 
