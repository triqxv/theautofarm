local function setPirate()
local args = {
    [1] = "SetTeam",
    [2] = "Pirates"
}
 
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer(unpack(args))
 
end
 
local function teleportMain()
local args = {
    [1] = "TravelMain"
}
 
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer(unpack(args))
end
 
local function teleportSecond()
    local args = {
        [1] = "Travel"
    }
 
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer(unpack(args))
end
 
local function teleportThird()
 
end
 
 
--introducing Tab
local Label = introducing:CreateLabel("Developer: shyde.dev")
local Label = introducing:CreateLabel("This is early access version of Tsuo Hub, there might be glitches.")
 
--Misc Tab
 
local Section = misc:CreateSection("Mirage:")
 
local notifyMirage = misc:CreateToggle({
    Name = "Notify if mirage spawned",
    CurrentValue = false,
    Flag = "automiragenotif", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function() end,
 })
 
 Run.Heartbeat:Connect(function ()
    if notifyMirage.CurrentValue then
        workspace.Map.DescendantAdded:Connect(function(mirage)
            if mirage.Name == "MysticIsland" then
                mirageCheck()
            end
        end)
    end
 end)
 
 local miragecheckButton = misc:CreateButton({
    Name = "Check Mirage",
    Callback = function()
        mirageCheck()
    end,
 })
 
 local TpToHighest = misc:CreateButton({
    Name = "Tp to highest point",
    Callback = function()
        if mirageStatus == true then
            highestMiragePoint()
        else
            mirageCheck()
        end
    end,
 })
 
 
 local Section = misc:CreateSection("Servers:")
 
 local hopServer = misc:CreateButton({
    Name = "Hop server",
    Callback = function()
        lowServer()
    end,
 })
 
 local rejoin = misc:CreateButton({
    Name = "Rejoin server",
    Callback = function()
        Teleport:Teleport(game.PlaceId, LP)
    end,
 })
 
 local Section = misc:CreateSection("Sea's:")
 
 local tpToFirstSea = misc:CreateButton({
    Name = "Teleport to First sea",
    Callback = function()
        task.wait(1)
        teleportFirst()
    end,
 })
 
 local tpToSecondSea = misc:CreateButton({
    Name = "Teleport to Second sea",
    Callback = function()
        task.wait(1)
        teleportSecond()
    end,
 })
 
 local tpToThirdSea = misc:CreateButton({
    Name = "Teleport to Third sea",
    Callback = function()
        task.wait(1)
        teleportThird()
    end,
 })
 
 local Section = misc:CreateSection("GUI:")
 local destroyButton = misc:CreateButton({
    Name = "Destroy GUI",
    Callback = function()
        Rayfield:Notify({
            Title = "Tsuo Hub",
            Content = "Are you sure you want to close GUI?",
            Duration = 6.5,
            Image = 9838873385,
            Actions = { 
               Yes = {
                  Name = "Yes",
                  Callback = function()
                  LP.PlayerGui.Main.Version.Text = originalText
                  Rayfield:Destroy()
               end
            },
                No = {
                    Name = "No",
                    Callback = function()
                        print('User resigned from closing gui')
                end
                },
         },
         })
    end,
 })
 
